{
  "$jason": {
    "head": {
      "title": "Calculator APP4WEB",
      "styles": {
        "bold": {
          "font": "HelveticaNeue-CondensedBold",
          "size": "15"
        },
        "padded": {
          "padding": "10"
        },
        "normal": {
          "font": "HelveticaNeue",
          "size": "12"
        }
      },
      "actions": {
        "$foreground": {
          "type": "$reload"
        }
      }
    },
    "body": {
      "header": {
        "title": "CALCULATION APP4WEB",
        "style": {
          "background": "#0000CD",
          "color": "#ffffff",
          "font": "AvenirNext-Bold",
          "size": "14"
        }
      },
      "sections": [
        {
          "header": {
            "type": "vertical",
            "style": {
              "background": "#ebebeb"
            },
            "components": [{
              "type": "label",
              "class": "bold",
              "text": "Data"
            }, {
              "type": "label",
              "class": "normal",
              "text": "Which 3rd party services do you need to integrate with?"
            }]
          },
          "items": [
            {
              "type": "label",
              "text": "Social Networks",
              "class": "bold padded",
              "image": "https://DinaDurykina.github.io/icons8-операционная-система-android-24.png"
            },
            {
              "type": "label",
              "text": "Location",
              "class": "bold padded",
              "image": "https://DinaDurykina.github.io/icons8-ос-mac-24.png"
            },
            {
              "type": "label",
              "text": "Media",
              "class": "bold padded",
              "image": "https://DinaDurykina.github.io/icons8-windows-8-24.png"
            },
            {
              "type": "label",
              "text": "eCommerce",
              "class": "bold padded",
              "image": "https://DinaDurykina.github.io/icons8-шаблон-24.png"
            }
          ]
        },

        {
          "header": {
            "type": "vertical",
            "style": {
              "background": "#ebebeb"
            },
            "components": [{
              "type": "label",
              "class": "bold",
              "text": "Database"
            }, {
              "type": "label",
              "class": "normal",
              "text": "Where do you want to save your application data ?"
            }]
          },
          "items": [{
            "type": "label",
            "text": "A new database",
            "class": "bold padded",
            "image": "https://DinaDurykina.github.io/icons8-глобус-24.png"
          },
            {
              "type": "label",
              "text": "An Existing Database",
              "class": "bold padded",
              "image": "https://DinaDurykina.github.io/icons8-глобус-24.png"
            },
            {
              "type": "label",
              "text": "Cloud Database",
              "class": "bold padded",
              "image": "https://DinaDurykina.github.io/icons8-глобус-24.png"
            },
            {
              "type": "label",
              "text": "I'm not sure",
              "class": "bold padded",
              "image": "https://DinaDurykina.github.io/icons8-глобус-24.png"
            }]
        },

        {

          "header": {
            "type": "vertical",
            "style": {
              "background": "#ebebeb"
            },
            "components": [
              {
                "type": "label",
                "class": "normal",
                "text": "Select third-party services and storage method and click"
              },
              {
                "type": "label",
                "text": "NEXT",
                "class": "bold",
                "href": {
                  "url": "https://DinaDurykina.github.io/calc_Security_NoImage.json",
                  "transition": "modal"
                }
              }
            ]
          }

        }
      ]
    }
  }
}