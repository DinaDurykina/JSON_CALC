{
  "$jason": {
    "head": {
      "title": "BRAZUKA",
      "actions": {
        "$load": {
          "type": "$set",
          "options": {
            "bg": "#212121"
          },
          "success": {
            "type": "$render"
          }
        }
      },
      "templates": {
        "body": {
          "header": {
            "style": {
              "background": "#FF9800",
              "color": "#212121"
            },
            "title": {
              "type": "image",
              "style": {
                "align": "left",
                "left": "20",
                "width": "120",
                "height": "50"
              },
              "url": "https://DinaDurykina.github.io/Brazuka_Rebrand_shkolatansev_black.png"
            },
            "menu": {
              "text": "Dance School",
              "style": {
                "color": "#3F51B5",
                "font": "Roboto",
                "size": "20"
              },
              "action": {
                "type": "$util.toast",
                "options": {
                  "text": "Гамсоновский пер., дом 2с1 — Бизнес-центр «Центр Т»,\n3 этаж, помещение №403.\nм. Тульская (3 мин. пешком)\nС собой взять документ!"
                }
              }
            }
          },
          "style": {
            "background": "{{$get.bg}}"
          },
          "layers": [
            {
              "type": "button",
              "url": "https://s3.amazonaws.com/www.textcast.co/icons/camera%402x.png",
              "style": {
                "left": "20",
                "bottom": "20",
                "width": "30",
                "height": "30",
                "color": "#ffffff"
              },
              "action": {
                "type": "$snapshot",
                "success": {
                  "type": "$util.share",
                  "options": {
                    "items": [
                      {
                        "type": "image",
                        "data": "{{$jason.data}}"
                      }
                    ]
                  }
                }
              }
            },
            {
              "type": "label",
              "text": "Когда Вы танцевали последний раз?",
              "style": {
                "background": "rgba(0,0,0,0.7)",
                "color": "#ffffff",
                "padding": "10",
                "left": "10",
                "font": "Roboto",
                "size": "36",
                "bottom": "265"
              },
              "action": {
                "type": "$set",
                "options": {
                  "bg": "#ff0000"
                },
                "success": {
                  "type": "$render"
                }
              }
            },
            {
              "type": "label",
              "text": "На БЕСПЛАТНЫЙ урок >",
              "style": {
                "background": "#FF4081",
                "color": "#ffffff",
                "width": "310",
                "height": "50",
                "padding": "10",
                "left": "10",
                "font": "Roboto",
                "size": "20",
                "bottom": "170"
              },
              "action": {
                "type": "$set",
                "options": {
                  "bg": "#00ff00"
                },
                "success": {
                  "type": "$render"
                }
              }
            },
            {
              "type": "label",
              "text": "Расписание                       > ",
              "style": {
                "background": "#C5CAE9",
                "color": "#212121",
                "width": "310",
                "height": "50",
                "padding": "10",
                "left": "10",
                "font": "Roboto",
                "size": "20",
                "bottom": "105"
              },
              "action": {
                "type": "$set",
                "options": {
                  "bg": "http://i.giphy.com/Is0AJv4CEj9hm.gif"
                },
                "success": {
                  "type": "$render"
                }
              }
            }
          ],
          "footer": {
            "tabs": {
              "style": {
                "background": "#FF9800",
                "color": "#000000"
              },
              "items": [
                {
                  "image": "https://DinaDurykina.github.io/icons8-vk.com-24.png",
                  "url": "https://DinaDurykina.github.io/buyteh.json"
                },
                {
                  "image": "https://DinaDurykina.github.io/icons8-home-page-24.png",
                  "url": "https://DinaDurykina.github.io/bagteh.json"
                },
                {
                  "image": "https://DinaDurykina.github.io/icons8-marker-24.png",
                  "url": "https://DinaDurykina.github.io/truckteh.json"
                }
              ]
            }
          }
        }
      }
    }
  }
}